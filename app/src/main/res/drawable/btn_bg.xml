<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/not_pressed_icon" android:state_pressed="false">
        <shape>
            <corners android:radius="16dp" />
            <stroke android:width="2dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/pressed_icon" android:state_pressed="true">
        <shape>
            <corners android:radius="16dp" />
            <stroke android:width="2dp" />
        </shape>
    </item>
</selector>